<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/extrauma_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Extrauma"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bonone_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bon One"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/extrauma_button" />

    <Button
        android:id="@+id/ophtatrov_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ophtatrov"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bonone_button" />

</androidx.constraintlayout.widget.ConstraintLayout>